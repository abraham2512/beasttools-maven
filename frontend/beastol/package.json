{
  "name": "beastol",
  "version": "1.0.0",
  "description": "Beast OpenLayers with Parcel",
  "scripts": {
    "start": "parcel serve index.html",
    "clean": "rm ../../src/main/resources/static/*",
    "build": "export NODE_OPTIONS=--no-experimental-fetch && parcel build --public-url . index.html --dist-dir ../../src/main/resources/static/"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "bootstrap": "^5.1.3",
    "jquery": "^3.6.0",
    "ol": "latest"
  },
  "devDependencies": {
    "parcel": "^2.0.0",
    "parcel-bundler": "^1.3.1"
  },
  "browserslist": ["last 10 chrome versions"]
}
